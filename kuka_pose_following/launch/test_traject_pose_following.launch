<launch>
  <env name="ROSCONSOLE_CONFIG_FILE"
       value="$(find kuka_pose_following)/config/logger.conf"/>
  <include file="$(find kuka_pose_following)/launch/pose_following.launch" />
  <node pkg="kuka_pose_following" type="test_traject" name="test_traject"
        output="screen">
    <rosparam>
      rate: 40
    </rosparam>
    <remap from="pose" to="pose_following/pose" />
  </node>
</launch>
