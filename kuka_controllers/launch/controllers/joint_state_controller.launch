<launch>

	<!-- Load joint controller configurations from YAML file to parameter server -->
	<rosparam file="$(find kuka_controllers)/config/kuka_controllers.yaml" command="load"/>
	
	<node ns="/KUKA" name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
		output="screen"  args="joint_state_controller"/> 
	
</launch>
